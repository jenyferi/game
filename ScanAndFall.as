package  {		public class ScanAndFall {				private var _scanner:Scanner;		private var _fallLength:int;		public function ScanAndFall(scanner:Scanner, fallLength:int) {			_scanner = scanner;			_fallLength = fallLength;		}				public function get scanner():Scanner{			return _scanner;		}				public function get fallLength():int {			return _fallLength;		}				public function incrementFallLength():void {			_fallLength++;		}	}	}