package  {		import flash.display.MovieClip;	import View;        public class Main extends MovieClip {		private var _view:View;		private var _controller:Controller;		private var _model:Model;		        public function Main() {			_model = new Model();			_controller = new Controller(_model);            _view = new View(_controller, _model);						_model.createBoard();			_controller.addMouseEvents();			_view.displayBoard();			            addChild(_view);        }    }}